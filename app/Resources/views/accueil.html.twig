              				{% for cat in categories %}
								<li><a href="categories/{{ cat.id }}">{{ cat.nom }}</a></li>
							{% endfor %}
							</ul>
            			</li>
						<li><a href="#contact">Vente privée</a></li>
          			</ul>
          			<ul class="nav navbar-nav navbar-right">
