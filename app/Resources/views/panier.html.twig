<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title>{% block title %}{% endblock %}</title>
	<link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
	<link href="{{ asset('css/panier.css') }}" rel="stylesheet" media="screen">
	<script src="{{ asset('js/ie-emulation-modes-warning.js') }}"></script> 
	{% block stylesheets %}{% endblock %}
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">Panier</a>
            </div>
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="{{ path('site.index') }}">Accueil</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    {% block breadcrumb %}{% endblock %}
		<div class="container text-center">
			{% block details %}{% endblock %}
			{% block quantite %}{% endblock %}
			{% block commandes %}{% endblock %}	
			{% block confirm %}{% endblock %}			
		</div>	
		{% block achat %}{% endblock %}	
		{% block edit %}{% endblock %}
    <script src="{{ asset('js/jquery.js') }}"></script>
	<script src="{{ asset('js/bootstrap.min.js') }}"></script> 
	<script src="{{ asset('js/panier.js') }}"></script> 
	<script src="{{ asset('js/customjs.js') }}"></script> 
	{% block javascripts %}{% endblock %}
  </body>
</html>
