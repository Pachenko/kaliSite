<!-- CAROUSEL -->
<div id="myCarousel" class="carousel slide" data-ride="carousel">
<!-- Indicators -->
	<ol class="carousel-indicators">
		<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
		<li data-target="#myCarousel" data-slide-to="1"></li>
		<li data-target="#myCarousel" data-slide-to="2"></li>
	</ol>
	
	<div class="carousel-inner">
		<div class="item active">
			<img class="featurette-image img-responsive" heigth="500px" width="1000px" src="{% block slider1 %}{% endblock %}" alt="First slide" />
			<div class="container">
				<div class="carousel-caption">
					<a href="{% block url_slider1 %}{% endblock %}"><img class="featurette-image img-responsive" heigth="500px" width="1000px" src="{{ asset('img/transparente.png')}}" alt="First slide" /></a>
				</div>
			</div>
		</div>
		
		<div class="item">
			<img class="featurette-image img-responsive" heigth="500px" width="1000px" src="{% block slider2 %}{% endblock %}" alt="Second slide" />
